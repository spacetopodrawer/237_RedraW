{
  "name": "237-redraw",
  "version": "1.0.0",
  "description": "Application de post-traitement GNSS multiplateforme",
  "type": "module",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build": "npm run build:client && npm run build:server",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest",
    "test:db": "tsx server/test-connection.ts",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "autoprefixer": "^10.4.16",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.303.0",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
