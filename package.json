    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "vite": "^5.0.8",
    "vitest": "^1.2.0"
